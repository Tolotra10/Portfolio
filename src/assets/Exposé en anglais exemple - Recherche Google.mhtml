.header_fixed .search_button::before { width: 15px; height: 15px; content: ""; display: block; position: relative; top: 2px; background: url("https://www-pimido-com.cdn.ampproject.org/ii/AW/s/www.pimido.com/image/common/header_sprite.png") 0px 0px no-repeat; }

.header_fixed .header_right_section { float: right; height: 87px; }

.header_fixed .subscription_plans, .header_fixed .contact_link, .header_fixed .connect_link { float: left; text-align: center; position: relative; line-height: 18px; }

.header_fixed .contact_link a { color: rgb(255, 255, 255); font-family: robotoregular; font-size: 13px; line-height: 18px; }

.header_fixed .contact_link a:hover { text-decoration: none; color: rgb(211, 58, 109); }

.header_fixed .contact_link { padding: 0px 60px 0px 45px; }

.header_fixed .connect_link { cursor: pointer; position: relative; }

.header_fixed .connect_link span { color: rgb(255, 255, 255); }

@media (min-width: 992px) and (max-width: 1199px) {
  .header_fixed .search_box { margin-left: 5px; }
}

@media screen and (max-width: 1199px) {
  .header_fixed .contact_link { padding: 0px 20px 0px 0px; }
}

@media screen and (max-width: 991px) {
  .header_fixed .logo_all { width: 100px; }
  .header_fixed .search_box { width: 300px; }
  .header_fixed .contact_link { display: none; }
  .header_fixed .subscription_plans a, .header_fixed .contact_link a { font-size: 13px; }
  .header_fixed .contact_link { padding: 0px 15px 0px 10px; }
}

@media screen and (max-width: 767px) {
  .header_fixed .logo_all { margin: 0px 10px 0px 0px; }
  .header_fixed .search_box { float: none; width: 100%; clear: both; margin: 0px; }
  .header_fixed .header_right_section { position: absolute; right: 10px; top: 10px; height: auto; }
  .header_mini .mobile_menu, .header_fixed .mobile_menu { display: block; float: left; }
  .header_fixed .search_box input[type="text"] { padding: 5px 35px 5px 15px; font-size: 12px; }
  .header_fixed .subscription_plans, .header_fixed .contact_link { line-height: 16px; }
  .header_fixed .connect_link .user_name, .header_fixed .contact_link { display: none; }
  .header_fixed .header_top { padding-bottom: 10px; }
  .header_fixed .search_button { top: 5px; right: 10px; }
}

.header_fixed { position: relative; padding: 10px; }

.header_fixed .logo { width: 115px; float: left; margin-bottom: 10px; }

.logo img { width: 90%; }

.login-register { color: rgb(255, 255, 255); float: right; font-size: 12px; font-weight: 700; margin: 8px 15px 0px 0px; padding: 6px 7px; text-decoration: none; border: 1px solid rgb(255, 255, 255); background: rgb(56, 56, 56); }

.login-name { float: right; font-size: 12px; color: rgb(211, 58, 109); font-weight: 700; margin: 14px 10px 0px 0px; }

.c-hamburger { border: medium none; cursor: pointer; display: block; height: 30px; float: right; margin: 0px; background: rgb(56, 56, 56); position: relative; text-indent: -9999px; width: 30px; }

.c-hamburger span { background: none 0px 0px repeat scroll rgb(255, 255, 255); display: block; height: 4px; left: 0px; position: absolute; right: 0px; top: 20px; }

.c-hamburger span::before { top: -10px; }

.c-hamburger span::after { bottom: -10px; }

.c-hamburger span::after, .c-hamburger span::before { background-color: rgb(255, 255, 255); content: ""; display: block; height: 4px; left: 0px; position: absolute; width: 100%; }

.searchbar { position: relative; clear: both; border: 1px solid rgb(229, 229, 229); width: 100%; }

.searchbar input[type="text"] { border: none; width: 98%; display: block; margin: 5px auto; }

amp-sidebar { width: 250px; padding-right: 10px; }

.amp-sidebar-image { line-height: 100px; vertical-align: middle; }

.amp-close-image { top: 0px; left: 0px; background: rgb(213, 39, 32); cursor: pointer; }

.lightbox { background: rgba(0, 0, 0, 0.8); width: 100%; height: 100%; position: absolute; }

.lightbox h1 { color: rgb(255, 255, 255); }

.amp-carousel-button-prev, .amp-carousel-button-next { display: none; }

.right-slider { width: 100%; position: fixed; height: 100%; background: rgb(56, 56, 56); }

.right-slider ul { margin: 0px; padding: 0px 20px; }

.right-slider ul li { border-bottom: 1px solid rgb(85, 85, 85); display: block; list-style: outside none none; margin: 0px; padding: 10px 0px; }

.right-slider ul li a { color: rgb(255, 255, 255); font-size: 12px; text-decoration: none; padding: 0px; text-transform: uppercase; margin: 0px; }

.right-slider ul ul li a { font-size: 11px; }

.height-text-label { font-size: 0px; line-height: 0; color: transparent; position: absolute; }

.mobile_menu { width: 20px; float: right; }
